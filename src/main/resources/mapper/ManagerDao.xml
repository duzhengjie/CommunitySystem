<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.communitySystem.dao.user.Manager.ManagerDao">

    <select id="getAll" resultType="Gonggao">
        SELECT id,title,content,date,manager_name managerName FROM gonggao ORDER BY date DESC;
    </select>

</mapper>